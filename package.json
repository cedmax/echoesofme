{
  "name": "echoesofme",
  "version": "1.0.0",
  "scripts": {
    "start": "node server.js",
    "watch": "node dev.js",
    "lint": "esw --ignore-pattern legacy --color --watch --fix api assets src",
    "bundle": "NODE_ENV=\"production\" webpack -p --config webpack.config.js",
    "test": "esw --color --fix src test && jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src",
      "assets"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cedmax/echoesofme.git"
  },
  "homepage": "http://obk.it",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-config-standard-jsx": "^3.3.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.1.1",
    "eslint-watch": "^3.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^1.14.0",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^1.14.1"
